{
    "server": {
        "host": "0.0.0.0",
        "port": 8080,
        "threads": 8
    },
    "paths": {
        "checkpoints": "/path/to/checkpoints",
        "diffusion_models": "/path/to/diffusion_models",
        "vae": "/path/to/vae",
        "lora": "/path/to/loras",
        "clip": "/path/to/clip",
        "t5": "/path/to/t5xxl",
        "embeddings": "/path/to/embeddings",
        "controlnet": "/path/to/controlnet",
        "llm": "/path/to/llm",
        "esrgan": "/path/to/esrgan",
        "taesd": "/path/to/taesd",
        "output": "/path/to/output",
        "webui": ""
    },
    "sd_defaults": {
        "n_threads": 10,
        "keep_clip_on_cpu": false,
        "keep_vae_on_cpu": false,
        "flash_attn": true,
        "offload_to_cpu": false
    },
    "preview": {
        "enabled": true,
        "mode": "tae",
        "interval": 1,
        "max_size": 256,
        "quality": 75
    },
    "assistant": {
        "enabled": false,
        "endpoint": "http://localhost:11434",
        "api_key": "",
        "model": "llama3.2",
        "temperature": 0.7,
        "max_tokens": 2000,
        "timeout_seconds": 120,
        "system_prompt": "",
        "max_history_turns": 20,
        "proactive_suggestions": true
    },
    "recycle_bin": {
        "enabled": true,
        "retention_minutes": 10080
    }
}
